<h1>Posts</h1>

<table = border ="1">
<tr>
    <td> Title </td>
    <td> Content</td>
    <tr>
    <% @posts.each do |f|%>
        <tr>
            <td><%= f.title %></td>
            <td><%= f.content %></td>
            <td><%=link_to("show",post_path(f.id))%></td>
            <td><%=link_to("edit",edit_post_path(f.id))%></td>
            <td><%= link_to 'delete', post_path(f.id), method: :delete, data: {:confirm => 'Emin misiniz?'} %></td>

        </tr>
    
    <% end %>

